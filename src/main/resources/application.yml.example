spring:
  datasource:
    driverClassName: com.mysql.cj.jdbc.Driver
    url: jdbc:mysql://:3306/<DATABASE_NAME>
    username: <USER_NAME>
    password: <PASSWORD>
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQLDialect

application:
  security:
    jwt:
      secret-key: 404E635266556A586E3272357538782F413F4428472B4B6250645367566B5970
      expiration: 300000
      refresh-token:
        expiration: 86400000
  user:
    admin:
      first-name: <ADMIN_USER_FIRST_NAME>
      last-name: <ADMIN_USER_LAST_NAME>
      email: <ADMIN_USER_EMAIL>
      password: <ADMIN_USER_PASSWORD>
    manager:
      first-name: <MANAGER_USER_FIRST_NAME>
      last-name: <MANAGER_USER_LAST_NAME>
      email: <MANAGER_USER_EMAIL>
      password: <MANAGER_USER_PASSWORD>
